programa 
{
	funcao inicio()
	{
		
	logico A, B
	
	escreva("Escreva um Verdadeiro ou Falso \n")
	leia(A)
	
	escreva("Escreva um Verdadeiro ou Falso \n")
	leia(B)


		se (A==verdadeiro e B==verdadeiro)
		{
		escreva("A ou B são verdadeiros" )
		}
		
		senao se (A==falso e B==falso)
		{
		escreva("A ou B são falsos")
		}
		
		senao
		{
			escreva("Um valor e verdadeiro e outro falso")
		}
		
		
	}
}



