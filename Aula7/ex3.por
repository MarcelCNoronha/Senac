programa 
{
	funcao inicio()
	{
		inteiro numero
		
		escreva ("Digite um número \n")
		leia (numero)
		
		escolha (numero)
		{
			caso 0: 
			escreva ("Você digitou o número 0 \n")
			pare
			
			
			caso 1: 
			escreva ("Você digitou o número 1 \n")
			pare
			
			
			caso 2: 
			escreva ("Você digitou o número 2 \n")
			pare
			
			caso contrario:
			escreva ("O valor não e igual a 0, 1 ou 2")
		}
		
		
		
	}
}
